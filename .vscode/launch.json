  "version": "0.2.0",
  "configurations": [
    {
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "name": "HomeRentals Firefox",
      "url": "http://localhost:8080/",
      "webRoot": "${workspaceFolder}/rentals/ruter"
    },
    {
      "name": "HomeRentals Server",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/rentals/main.py"
    }
  ],
  "compounds": [
    {
      "name": "HomeRentals",
      "configurations": ["HomeRentals Server", "HomeRentals Firefox"],
      "stopAll": true
    }
  ]
}
